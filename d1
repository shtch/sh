CREATE table PS_D_SERVICE_PACTYPE (
    SERVICE_PACTYPE_ID NUMBER(18,0) NOT NULL,
    NAME               VARCHAR2(128) NOT NULL,
    SORDER             NUMBER(8,0) NOT NULL,
    ACTIVE             NUMBER(1,0) NOT NULL,
    DATE_CREATED       DATE default sysdate,
    constraint PS_D_SERVICE_PACTYPE_PK primary key (SERVICE_PACTYPE_ID),
    constraint PS_D_SERVICE_PACTYPE_UK1 unique (NAME));
   
CREATE TABLE  PS_PRICE_LST
   (	PRICE_LST_ID NUMBER(18,0) 
       GENERATED BY DEFAULT ON NULL AS IDENTITY MINVALUE 1 MAXVALUE 9999999999999999999999999999
       INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOT NULL ENABLE, 
	NAME VARCHAR2(128) NOT NULL ENABLE, 
	SORDER NUMBER(8,0) NOT NULL ENABLE, 
	ACTIVE NUMBER(1,0) NOT NULL ENABLE, 
	DATE_START DATE NOT NULL ENABLE, 
	DATE_CREATED DATE NOT NULL ENABLE, 
	USER_CREATED NUMBER(18,0), 
	 CONSTRAINT PS_PRICE_LST_PK PRIMARY KEY (PRICE_LST_ID) USING INDEX  ENABLE, 
	 CONSTRAINT PS_PRICE_LST_UK1 UNIQUE (NAME)  USING INDEX  ENABLE, 
	 CONSTRAINT PS_PRICE_LST_UK2 UNIQUE (SORDER)
  USING INDEX  ENABLE
   )
/
